        map:uiZoomControls="true"
        map:uiRotateGestures="true"
        tomtom:mapStyleSource="STYLE_MERGER"
        tomtom:mapsApiKey="BmZwNYH2ZCio8OzotAg0KHl8iyOYe7u3"
